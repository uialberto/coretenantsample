@model IEnumerable<Producto>

@{
    ViewData["Title"] = "Home Page";
}


@using AppWeb.Entities
@using AppWeb.Helpers;

<div class="text-center">
       @*<h1 class="display-4">Welcome @HttpContextExtensions.GetTenant(@HttpContext).Items["Name"]</h1>*@
       <h1 class="display-4">Welcome @ViewData["TenantName"]  </h1>
       <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>

       <h1>Productos</h1>

<table class="table">
    <thead>
        <tr>
            <th>Product Id</th>
            <th>Description</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var product in Model)
        {
            <tr>
                <td>@product.ProductId</td>
                <td>@product.Description</td>
            </tr>
        }
    </tbody>
</table>

</div>
